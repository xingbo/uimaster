            <xs:extension base="Page:EventListenerType"/>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="KeyDownListenerType">
        <xs:complexContent>
            <xs:extension base="Page:EventListenerType"/>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="KeyUpListenerType">
        <xs:complexContent>
            <xs:extension base="Page:EventListenerType"/>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="ItemChangeListenerType">
